//		brisemer surface: @isOneOf(man_made,breakwater,groyne) //ne fonctionne pas, pas la possibilité de trouver area[] AND NOT way[]
		ponton: man_made=pier AND NOT area=no
		mur surface: @isOneOf(barrier,city_wall,wall) AND area=yes
		parcnat national: boundary=national_park
		parcnat protege: boundary=protected_area OR (leisure=nature_reserve AND NOT @isOneOf(boundary,protected_area,national_park))

properties
	user.use-metric: true		
	map.decoration.scale: true
	
rules
	target: ville
		define
			font-family: Open Sans
			font-weight: bold
			font-size: 15
			text-halo-width: 1
			text-opacity: 1
			text-halo-opacity: 0.7
			font-stretch: 0.95
			text: @up(name)
			placement-value: 1000
		draw: text
	target: village
		define
			font-family: Open Sans
			font-weight: bold
			font-size: 14
			text-line-spacing: 0.9
			text-halo-width: 1
			text-opacity: 1
			text-halo-opacity: 0.7
			font-stretch: 0.95
			text: name
			placement-value: 100
		for: place=hamlet
			define
				font-size: 12
				placement-value: 90
		draw: text
	target: quartier
		define
			text: @up(name)
			font-family: Open Sans
			font-weight: bold
			font-style: italic
			font-size: 12
			font-stretch: 0.7
			text-letter-spacing: -0.05
			text-line-spacing: 0.9
			text-halo-width: 1.2
			text-opacity: 1
			text-halo-opacity: 0.7
			placement-value: 99.9
		draw: text
	target: hameau
		define
			font-family: Open Sans
			font-style: italic
			font-size: 10
			text-line-spacing: 0.8
			text-halo-width: 0.5
			text-opacity: 1
			text-halo-opacity: 0.5
			font-stretch: 0.85
			text: name
			placement-value: 5
		for: place=isolated_dwelling
			define
				placement-value: 4.9
		draw: text
	target: lieudit
		define
			font-family: Open Sans
			font-style: italic
			font-size: 9
			text-line-spacing: 0.8
			font-stretch: 0.75
			text-halo-width: 0.4
			text-opacity: 1
			text-halo-opacity: 0.6
			text: name
			placement-value: 0.1
		draw: text
	
	target: point GPS
		define
			icon-image: R25/icons/Perso/etoilef1_tr.png
			icon-width: 12
		draw: icon
		define
			text: name
			font-family: DejaVu Sans Condensed
			text-opacity: 0.9
			font-style: italic
			font-weight: bold
			text-color: fuchsia black 10%
			font-stretch: 0.85
			text-halo-width: 0.8
			text-halo-opacity: 0.5
			text-line-spacing: 0.8
			text-align-horizontal: near
			text-offset-horizontal: 5
			text-offset-vertical: -5
			placement-value: 99
		draw: text

	target: ecole
		define
			font-family: DejaVu Sans Condensed
			font-style: italic
			font-weight: bold
			text-color: yellow brown 50%
			text-color: #800000
			font-stretch: 0.9
			text-letter-spacing: -0.03
			font-size: 9
			text-opacity: 0.9
			text-halo-width: 1
			text-halo-color: white
			text-halo-opacity: 0.5
			text-line-spacing: 0.8
			text-align-horizontal: center
		for: @isOneOf(school:FR,maternelle,"élémentaire",primaire)
			define
				text: "Éc."
				placement-value: 1.5
		elsefor: school:FR="collège"
			define
				text: "Coll."
				placement-value: 2.5
		elsefor: @isOneOf(school:FR,"lycée",secondaire)
			define
				text: "Lyc."
				placement-value: 3.5
		elsefor: @isOneOf(amenity,university,college)
			define
				text: "Univ."
				placement-value: 3.5
		elsefor: @isOneOf(amenity,kindergarten,school)
			define
				text: "Éc."
				placement-value: 1
		else
			stop
		draw: text

		
	target: office de tourisme
		define
			shape: square
