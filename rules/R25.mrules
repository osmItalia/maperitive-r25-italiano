		zone militaire: @isOneOf(military, danger_area, naval_base, range) OR landuse=military
		lac: natural=water OR @isOneOf(waterway,riverbank,dock) OR @isOneOf(landuse,reservoir,basin)
		barrage surfacique: waterway=dam
		cimetiere: landuse=cemetery OR amenity=grave_yard
