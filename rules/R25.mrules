features
	attraction: node[tourism=attraction OR @isOneOf(historic,monument,city_gate,castle,fort)] area[tourism=attraction OR @isOneOf(historic,monument,city_gate,castle,fort)]
	camping: node[tourism=camp_site] area[tourism=camp_site]
	gite: node[@isOneOf(tourism,alpine_hut,hostel,guest_house)] area[@isOneOf(tourism,alpine_hut,hostel,guest_house)]
	cabane: node[tourism=wilderness_hut] area[tourism=wilderness_hut]
	abri: node[amenity=shelter AND tourism!=wilderness_hut AND shelter_type!= public_transport] area[amenity=shelter AND tourism!=wilderness_hut AND shelter_type!= public_transport]
	eglise: node[amenity=place_of_worship OR @isOneOf(building,church,chapel,cathedral)] area[amenity=place_of_worship OR @isOneOf(building,church,chapel,cathedral)]
	eau potable: node[amenity=drinking_water OR @isOneOf(drinkable,yes,mineral) OR drinking_water=yes] area[amenity=drinking_water OR @isOneOf(drinkable,yes,mineral) OR drinking_water=yes]
	source: node[(natural=spring OR amenity=fountain OR amenity=watering_place) AND NOT @isOneOf(drinkable,yes,mineral) AND NOT amenity=drinking_water] area[(natural=spring OR amenity=fountain OR amenity=watering_place) AND NOT @isOneOf(drinkable,yes,mineral) AND NOT amenity=drinking_water]
	rocher: node[natural=stone] area[natural=stone]
	parking: node[amenity=parking AND NOT @isOneOf(access,no,private,customer,customers)] area[amenity=parking  AND NOT @isOneOf(access,no,private,customer,customers)]
	hopital: node[amenity=hospital] area[amenity=hospital]
	office de tourisme: node[tourism=information AND information=office] area[tourism=information AND information=office]
	musee: node[(@isOneOf(tourism,museum,zoo) OR historic=farm) AND NOT(tourism=attraction OR @isOneOf(historic,monument,city_gate) OR @isOneOf(historic,castle,fort))] area[(@isOneOf(tourism,museum,zoo) OR historic=farm) AND NOT(tourism=attraction OR @isOneOf(historic,monument,city_gate) OR @isOneOf(historic,castle,fort))]
	parc d attraction: node[tourism=theme_park] area[tourism=theme_park]
	station_remontepente: node[aerialway=station] 
	calvaire: node[@isOneOf(historic, wayside_cross, wayside_shrine, rune_stone)] area[@isOneOf(historic, wayside_cross, wayside_shrine, rune_stone)]
	pylone remontepente: way[aerialway AND aerialway!=platter].node[aerialway=pylon]
	grue: node[man_made=crane] area[man_made=crane]
	balise maritime special: node[man_made=lighthouse] area[man_made=lighthouse]
	mine: node[man_made=mineshaft] area[man_made=mineshaft]
	grotte: node[natural=cave_entrance] area[natural=cave_entrance]
	volcan: node[natural=volcano AND NOT natural=peak] area[natural=volcano AND NOT natural=peak]
	baie: node[natural=bay] area[natural=bay]
	reservoirstockage: node[man_made=storage_tank] area[man_made=storage_tank AND NOT building]
	station epuration: node[@isOneOf(man_made,wastewater_plant,water_works)] area[@isOneOf(man_made,wastewater_plant,water_works)]
	moulin a vent: node[man_made=windmill] area[man_made=windmill]
	eolienne: node[generator:source=wind] area[generator:source=wind]
	tour: node[@isOneOf(man_made,tower,lighthouse)] area[@isOneOf(man_made,tower,lighthouse)]
	chateau deau: node[man_made=water_tower] area[man_made=water_tower]
	table train: node[railway=turntable] area[railway=turntable]
	douane: node[barrier=border_control] area[barrier=border_control]
	peage: node[barrier=toll_booth] area[barrier=toll_booth] 
	mairie: node[amenity=townhall] area[amenity=townhall] 
	plage gardee: node[leisure=beach_resort] area[leisure=beach_resort] 
	patinoire: node[leisure=ice_rink] area[leisure=ice_rink] 
	minigolf: node[leisure=miniature_golf] area[leisure=miniature_golf] 
	piscine: node[(leisure=swimming_pool OR amenity=swimming_pool)] area[(leisure=swimming_pool OR amenity=swimming_pool)] 
	jeux deau: node[leisure=water_park] area[leisure=water_park] 
	sport: node[@isOneOf(leisure,pitch,sports_centre,stadium,track)] area[@isOneOf(leisure,pitch,sport_center,stadium,track)] 
	escalade: node[sport=climbing] way[sport=climbing]
	police: node[amenity=police] area[amenity=police]
	pompier: node[amenity=fire_station] area[amenity=fire_station]
	ecole: node[@isOneOf(amenity,college,kindergarten,school,university)] area[@isOneOf(amenity,college,kindergarten,school,university)]
	divers ponctuel surface: area[@isOneOf(leisure,bird_hide,wildlife_hide,hunting_stand,playground)]
//	port plaisance: node[leisure=marina] area[leisure=marina] 
	
	legende: node[legende:FR] way[legende:FR AND NOT area=yes] area [legende:FR AND area=yes]
//	legende: impression conseillée à l'échelle équivalent au 20000ème avec le fichier fourni
		
	points
		ville: @isOneOf(place, city, town)
		village: place=village OR (place=hamlet AND ref:INSEE)
		quartier: place=suburb
		hameau: @isOneOf(place,hamlet,isolated_dwelling,neighbourhood) AND NOT (place=hamlet AND ref:INSEE)
		lieudit: @isOneOf(place,locality,farm)
		sommet: natural=peak
		col: mountain_pass=yes OR (natural=saddle AND NOT mountain_pass=yes)
		arbre: natural=tree
		gare: (@isOneOf(railway,station,halt) AND @isOneOf(operator,SNCF,sncf,Sncf)) OR amenity=ferry_terminal
		arret tram: railway=tram_stop
		entree metro: railway=subway_entrance
		
		altitude: man_made=survey_point AND ele AND NOT @isMatch(ref," - 2| - 3| - 4| - 5| - B| - C| - D| - E")
		borne: historic=boundary_stone
		point de vue: tourism=viewpoint
		bateau: @isOneOf(historic,wreck,ship)
		
		pylone electrique: power=tower
		barrage travers: node[waterway=weir]
		ecluse: waterway=lock_gate OR lock=yes
		
		entree mine: man_made=adit
		balise maritime: man_made=beacon OR seamark=beacon OR airmark=beacon OR @isOneOf(seamark:type, light_major, light_minor, light_vessel, light_float,  buoy_cardinal, buoy_installation, buoy_isolated_danger, buoy_lateral, buoy_safe_water, buoy_special_purpose)
		cheminee: man_made=chimney
		puitpetrole: man_made=petroleum_well
		bloc: @isOneOf(barrier,block,bollard,debris,full-height_turnstile,gate,horse_stile,jersey_barrier,kissing_gate,lift_gate,log,stile,swing_gate,turnstile)
		
		divers ponctuel: @isOneOf(man_made,flagpole,kiln,monitoring_station,watermill,water_well) OR aeroway=windsock OR @isOneOf(leisure,bird_hide,wildlife_hide,hunting_stand,playground)

		point GPS: gpswaypoint[]
//		point GPS: node[legende=pointgps]
		
	lines
		autoroute: @isOneOf(highway, motorway, trunk) AND NOT area=yes
		bretelle autoroute: @isOneOf(highway, motorway_link, trunk_link) AND NOT area=yes
		route majeure: @isOneOf(highway,primary,primary_link) AND NOT area=yes
		route mineure: @isOneOf(highway,tertiary,secondary,tertiary_link,secondary_link) AND NOT area=yes
		route residentielle: @isOneOf(highway,unclassified,residential,road,living_street,pedestrian) AND NOT area=yes AND NOT lanes=1
		route residentielle etroite: @isOneOf(highway,unclassified,residential,road,living_street,pedestrian) AND NOT area=yes AND lanes=1
		route service: highway=service AND NOT area=yes

		railprincipal: @isOneOf(railway,rail,funicular,light_rail,monorail,narrow_gauge,preserved) AND ((NOT tunnel OR tunnel=no) OR NOT (@isOneOf(operator,RATP,ratp,Ratp))) AND (NOT service OR service=no)
		railsecondaire: @isOneOf(railway,disused,miniature,tram) OR (@isOneOf(railway,rail,funicular,light_rail,monorail,narrow_gauge,preserved) AND ((NOT tunnel OR tunnel=no) OR NOT (@isOneOf(operator,RATP,ratp,Ratp))) AND (service AND service!=no))

		track12: highway=track AND (tracktype=grade1 OR tracktype=grade2) AND NOT (@isOneOf(smoothness,very_bad,horrible,very_horrible,impassable))
		track3: highway=track AND (tracktype=grade3 OR ((tracktype=grade1 OR tracktype=grade2) AND @isOneOf(smoothness,very_bad,horrible))) AND NOT (@isOneOf(smoothness,very_horrible,impassable))
		track45: (highway=track AND ((tracktype!=grade3 AND tracktype!=grade2 AND tracktype!=grade1) OR (@isOneOf(smoothness,very_horrible,impassable)))) OR highway=bridleway
		sentier: @isOneOf(highway,footway,path,cycleway,steps) AND NOT footway=sidewalk
		layon: man_made=cutline
		ponton: man_made=pier OR leisure=slipway
		
		tunnel seul: tunnel=yes AND NOT highway AND NOT waterway AND NOT @isOneOf(railway,rail,funicular,light_rail,monorail,narrow_gauge,preserved,disused, miniature, tram) AND NOT (railway=subway OR @isOneOf(operator,RATP,ratp,Ratp)) AND NOT man_made=pipeline
		pont seul: bridge AND bridge!=no AND NOT highway AND NOT waterway AND NOT @isOneOf(railway,rail,funicular,light_rail,monorail,narrow_gauge,preserved,disused, miniature, tram) AND NOT aeroway AND NOT @isOneOf(aerialway,cable_car,chair_lift,drag_lift,gondola,goods,mixed_lift,platter)
		
		piste aterrissage: @isOneOf(aeroway,runway,taxiway)
		ferry: route=ferry

		fosse: waterway=ditch OR waterway=drain OR barrier=ditch
		ruisseau: waterway=stream
		riviere: waterway=river OR waterway=canal
		barrage lineaire: @isOneOf(waterway,dam,weir)
		
		remontepente: @isOneOf(aerialway,cable_car,chair_lift,drag_lift,gondola,goods,mixed_lift,platter)
		ligne electrique: power=line
		pipeline: man_made=pipeline AND NOT location=underground AND NOT tunnel=yes

		rangeearbres: natural=tree_row
		brisemer: way[@isOneOf(man_made,breakwater,groyne)] 
		leveeterre: man_made=embankment AND (NOT highway) AND (NOT railway) AND (NOT waterway)
		tranchee: cutting=yes AND (NOT highway) AND (NOT railway) AND (NOT waterway)
		levee: embankment=yes AND (NOT highway) AND (NOT railway) AND (NOT waterway)
		falaise: natural=cliff
		divers lineaire: @isOneOf(man_made,snow_fence,snow_net)
		
		mur: @isOneOf(barrier,city_wall,wall)
		cloture: barrier=fence
		haie: barrier=hedge OR natural=hedge
		soutainement: barrier=retaining_wall
		
		contour majeur : contour[@isMulti(elevation, 50) and not elevation=0]
		contour mineur : contour[@isMulti(elevation, 1) and not @isMulti(elevation, 50)]
		contour legende: @isOneOf(legende,contour_majeur,contour_mineur)

//		itineraire balise : relation[type=route AND (route=foot OR route=hiking) AND NOT name="Via Romea Francigena, French part"] way[(trailblazed=yes OR marked_trail=yes) AND NOT name="Via Romea Francigena, French part"]
//		itineraire balise : relation[type=route AND (route=foot OR route=hiking) AND NOT ref=HRP] way[(trailblazed=yes OR marked_trail=yes) AND NOT name="HRP"]
//		itineraire balise : relation[type=route AND (route=foot OR route=hiking) AND NOT @isOneOf(network,iwn,nwn)] way[(trailblazed=yes OR marked_trail=yes) AND NOT @isOneOf(network,iwn,nwn)]
		itineraire balise : relation[type=route AND (route=foot OR route=hiking)] way[(@isOneOf(trailblazed,yes,foot,hiking,major) OR @isOneOf(marked_trail,yes,foot,hiking,major) OR rendu=trailblazed_major)]
		
		trace GPS: gpstrack[] 
//		trace GPS: legende=tracegps

		limite administrative: boundary=administrative AND @isOneOf(admin_level,2,4,6,7,8)
		limite maritime: boundary=maritime
		
